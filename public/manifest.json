{
  "manifest_version": 3,
  "short_name": "manifest-v3-react-rsbuild-template",
  "name": "manifest-v3-react-rsbuild-template",
  "browser_specific_settings": {
    "gecko": { "id": "@manifest-v3-react-rsbuild-template" }
  },
  "icons": {
    "16": "icons/16.png",
    "24": "icons/24.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "64": "icons/64.png",
    "96": "icons/96.png",
    "128": "icons/128.png"
  },
  "version": "0.0.1",
  "action": {
    "default_icon": {
      "16": "icons/16.png",
      "24": "icons/24.png",
      "32": "icons/32.png",
      "48": "icons/48.png",
      "64": "icons/64.png",
      "96": "icons/96.png",
      "128": "icons/128.png"
    },
    "default_popup": "static/html/popup.html",
    "default_area": "menupanel"
  },
  "options_ui": {
    "page": "static/html/options.html"
  },
  "permissions": ["scripting"]
}
